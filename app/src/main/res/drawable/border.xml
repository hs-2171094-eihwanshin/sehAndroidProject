<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="1dp"
        android:right="1dp"
        android:top="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle" >
            <stroke
                android:width="1dp"
                android:color="#303030" />
            <solid android:color="#88ff7f00" />
            <corners
                android:radius="20dp"/>
        </shape>
    </item>

</layer-list>